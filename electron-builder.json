{
    "productName": "AGRallye",
    "directories": {
        "output": "release/",
        "buildResources": "src/assets/icons/"
    },
    "files": [
        "dist/*",
        "main.js"
    ],
    "extraResources": [
        "src/assets/icons/icon.png"
    ],
    "win": {
        "icon": "dist/assets/icons",
        "target": [
            "nsis"
        ],
        "publish": ["github"]
    },
    "mac": {
        "icon": "dist/assets/icons",
        "target": [
            "dmg"
        ],
        "publish": ["github"]
    },
    "linux": {
        "icon": "dist/assets/icons",
        "target": [
            "AppImage"
        ],
        "publish": ["github"]
    }
}